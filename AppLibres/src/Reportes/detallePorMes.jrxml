	<queryString>
		<![CDATA[SELECT  cast(strftime('%m',fecha_emision) as numeric) as me,
cast(strftime('%m',fecha_emision) as numeric) as mes,
g.tipo AS tipo, cast(sum(g.total)as numeric) AS total
from factura f join tipo_gasto g on f.id_factura=g.id_factura where strftime('%Y',fecha_emision)=$P{Anio}
and f.tipo_factura=$P{tipo_factura}
group by MES,TIPO,ME order by 1]]>
	</queryString>
