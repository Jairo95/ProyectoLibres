	<queryString>
		<![CDATA[select g.tipo as tipo,cast(sum(g.total)as numeric) as total
from factura f join tipo_gasto g on f.id_factura=g.id_factura
where fecha_emision>= strftime('%Y-%m-%d',$P{fechaInicio}) and fecha_emision<strftime('%Y-%m-%d',$P{fechaFin})
and f.tipo_factura=$P{tipoFactura}
and f.id_cliente=$P{idCliente}
and f.id_establecimiento=(select id_establecimiento from establecimiento where nombre_establecimiento=$P{establecimiento})
group by g.tipo;]]>
	</queryString>
	<field name="tipo" class="java.lang.Object"/>
	<field name="total" class="java.lang.Object"/>
